            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                <subModelOperations xsi:type="tool_1:Switch">
                  <cases conditionExpression="aql:self.ownedBehavior->isEmpty() and self.referencedBehavior->isEmpty()">
                    <subModelOperations xsi:type="tool_1:CreateInstance" typeName="DataDictionaryCharacterized::BehaviorDefinition" referenceName="ownedBehavior" variableName="new behavior">
                      <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="new behavior"/>
                      <subModelOperations xsi:type="tool_1:Let" variableName="node" valueExpression="aql:self.eContainer()">
                        <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql:self.ownedBehavior">
                          <subModelOperations xsi:type="tool_1:CreateInstance" typeName="DataDictionaryCharacterized::Pin" referenceName="outputs">
                            <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="aql:node.name+'.out.newPin'"/>
                          </subModelOperations>
                        </subModelOperations>
                      </subModelOperations>
                    </subModelOperations>
                  </cases>
                  <cases conditionExpression="aql:self.ownedBehavior->notEmpty()">
                    <subModelOperations xsi:type="tool_1:Let" variableName="node" valueExpression="var:self">
                      <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql:self.ownedBehavior">
                        <subModelOperations xsi:type="tool_1:CreateInstance" typeName="DataDictionaryCharacterized::Pin" referenceName="outputs">
                          <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="aql:node.name+'.out.newPin'"/>
                        </subModelOperations>
                      </subModelOperations>
                    </subModelOperations>
                  </cases>
                  <default/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:DirectEditLabel" name="editNodeName" inputLabelExpression="aql:self.name">
            <mask mask="{0}"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="var:0"/>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:DirectEditLabel" name="editPinName" inputLabelExpression="aql:self.name">
            <mask mask="{0}"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="var:0"/>
            </initialOperation>
          </ownedTools>
        </toolSections>
