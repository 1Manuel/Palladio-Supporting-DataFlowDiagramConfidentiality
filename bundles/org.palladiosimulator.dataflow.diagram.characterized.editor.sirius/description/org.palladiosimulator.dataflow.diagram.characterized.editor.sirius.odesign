          <controls xsi:type="properties:LabelDescription" name="LHS Characteristic Type" labelExpression="Characteristic Type" valueExpression="aql:self.behavior.assignments.lhs.characteristicType.name"/>
        </controls>
        <controls xsi:type="properties:SelectDescription" name="Select Referenced Behavior">
          <initialOperation>
            <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:self"/>
          </initialOperation>
        </controls>
        <validationSet>
          <propertyValidationRules name="BehaviorNameValidation" level="WARNING" message="The name should not be empty." targets="//@extensions.0/@categories.0/@groups.3/@controls.0">
            <audits auditExpression="aql:self->size() > 0"/>
          </propertyValidationRules>
        </validationSet>
